{
  "name": "obsidian-cloudflare-plugin",
  "version": "1.0.1",
  "description": "This plugin uploads images from your clipboard to your own cloudflare bucket and embeds uploaded image to your note",
  "main": "main.js",
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node esbuild.config.mjs production",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint && npm run build"
  },
  "keywords": [
    "cloudflare",
    "clipboard",
    "obsidian",
    "plugin"
  ],
  "author": "youngseok.yoon",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "builtin-modules": "^3.3.0",
    "enquirer": "^2.4.1",
    "esbuild": "0.17.3",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "obsidian": "obsidianmd/obsidian-api",
    "obsidian-utils": "^0.10.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0"
  }
}
